{
  "name": "ansballard-ghpages",
  "version": "0.2.8",
  "description": "Github profile page for Aaron Ballard",
  "main": "index.html",
  "scripts": {
    "start": "http-server ./ -p 8080 -o",
    "cli": "node scripts/cli.js",
    "build:cli": "browserify --no-bundle-external --node ./src/scripts/cli.js -t [babelify --presets es2015] -o ./scripts/cli.js",
    "prepublish": "npm run build:cli && npm run cli -- -am"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ansballard/ansballard.github.com.git"
  },
  "author": "Aaron Ballard",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ansballard/ansballard.github.com/issues"
  },
  "homepage": "https://github.com/ansballard/ansballard.github.com#readme",
  "devDependencies": {
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "concurrently": "^2.0.0",
    "core-js": "^2.3.0",
    "cssnano": "^3.5.2",
    "denodeify": "^1.2.1",
    "glob": "^7.0.0",
    "highlight.js": "^9.2.0",
    "html-minifier": "^1.2.0",
    "http-server": "^0.9.0",
    "keypress": "^0.2.1",
    "marked": "^0.3.5",
    "mkdirp": "^0.5.1",
    "ora": "^0.2.1",
    "postcss": "^5.0.17",
    "postcss-cli": "^2.5.1",
    "postcss-cssnext": "^2.4.0",
    "postcss-import": "^8.0.2",
    "prompt": "^1.0.0",
    "rollup-plugin-babel": "^2.4.0",
    "rollup-plugin-commonjs": "^2.2.1",
    "rollup-plugin-json": "^2.0.0",
    "rollup-plugin-node-resolve": "^1.5.0",
    "rollupify": "^0.1.0",
    "uglify-js": "^2.6.2",
    "uncss": "^0.13.0",
    "watch": "^0.17.1"
  },
  "dependencies": {
    "page": "^1.6.4",
    "post-data": "file:./data"
  }
}
